  <rect x="60" y="90" width="20" height="20" rx="3" fill="#E8E8E5"/>
  <rect x="85" y="90" width="20" height="20" rx="3" fill="#E8E8E5"/>
  <rect x="110" y="90" width="20" height="20" rx="3" fill="#E8E8E5"/>

  <!-- Coffee mug suggestion -->
  <ellipse cx="200" cy="100" rx="35" ry="20" fill="#D4C4B0" opacity="0.7"/>
  <rect x="165" y="95" width="70" height="80" rx="5" fill="#FFFFFF" opacity="0.8"/>
  <path d="M 235 110 Q 250 110 250 130 Q 250 150 235 150" stroke="#D4C4B0" stroke-width="3" fill="none"/>

  <!-- Notebook suggestion -->
  <rect x="450" y="150" width="180" height="220" rx="5" fill="#FFFFFF" opacity="0.5"/>
  <line x1="470" y1="180" x2="610" y2="180" stroke="#E0E0DD" stroke-width="2"/>
  <line x1="470" y1="210" x2="610" y2="210" stroke="#E0E0DD" stroke-width="2"/>

  <!-- Main Text -->
