    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F5F5F0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FAFAF8;stop-opacity:1" />
