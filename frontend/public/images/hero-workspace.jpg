  <!-- Decorative elements -->
  <circle cx="1250" cy="120" r="60" fill="#FFFFFF" opacity="0.4"/>
  <circle cx="1200" cy="300" r="40" fill="#F0EDE8" opacity="0.5"/>
</svg>
